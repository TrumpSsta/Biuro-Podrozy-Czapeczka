@model IEnumerable<BiuroPodrozyCzapeczka.Models.Zwiedzanie>

@{
    ViewBag.Title = "Lista Zwiedzań";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Utwórz", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Miasto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CenaZwiedzania)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataZwiedzania)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Wycieczka.Panstwo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Miasto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CenaZwiedzania)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataZwiedzania)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wycieczka.Panstwo)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.IdZwiedzania }) |
            @Html.ActionLink("Właściwości", "Details", new { id=item.IdZwiedzania }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.IdZwiedzania })
        </td>
    </tr>
}

</table>
